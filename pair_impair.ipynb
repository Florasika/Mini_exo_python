    "\n",
    "#  Input utilisateur\n",
    "nombre = int(input(\"Entrez un nombre : \"))\n",
    "\n",
    "#  Conditions\n",
    "if nombre % 2 == 0:\n",
    "    resultat = \"pair\"\n",
    "else:\n",
    "    resultat = \"impair\"\n",
    "\n",
    "#  Formatage string\n",
    "print(f\"Le nombre {nombre} est {resultat} !\")"
